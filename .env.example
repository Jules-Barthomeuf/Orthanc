# Example environment variables
# Copy this to .env.local and fill in the values

# Database (currently using mock data)
DATABASE_URL=

# Authentication
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Feature Flags
NEXT_PUBLIC_ENABLE_BLOCKCHAIN=false
NEXT_PUBLIC_ENABLE_AI_ASSISTANT=true

# To enable Supabase storage, set STORAGE_PROVIDER to 'supabase' and provide credentials
# STORAGE_PROVIDER can be 'local' (default) or 'supabase'
STORAGE_PROVIDER=
SUPABASE_URL=
SUPABASE_KEY=
